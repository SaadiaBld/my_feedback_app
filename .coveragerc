# .coveragerc: Configuration file for coverage.py to specify which files to include or exclude from coverage reports.

[run]
omit =
    */__init__.py
    run.py
    start.sh
    config.py
    tests/*
    */templates/*
    */__pycache__/*
    */gcp-creds.json
    create_users.py
    .venv/*
    venv/*
    credentials/*

[report]
show_missing = true
skip_covered = true
